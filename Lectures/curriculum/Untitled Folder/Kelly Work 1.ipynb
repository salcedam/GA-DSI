   "source": [
    "def maximizer(list):\n",
    "    list.sort(key=(lambda x:(x**2)**.5), reverse=True)\n",
    "    for i in range(len(list)-2):\n",
    "        if list[i]*list[i+1]*list[i+2]>0:\n",
    "            return list[i]*list[i+1]*list[i+2]\n",
    "    return list[-1]*list[-2]*list[-3]"
   ]
  },
